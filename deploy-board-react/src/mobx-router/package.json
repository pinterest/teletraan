{
  "name": "mobx-router",
  "version": "0.1.1",
  "description": "A flexible router based on MobX",
  "main": "src/index.js",
  "files": [
    "dist/",
    "src/",
    "test/",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "lint": "eslint '{src,tests}/**/*.js'",
    "test": "jest",
    "build": "del-cli dist && npm run lint && babel src -d dist",
    "format": "prettier --print-width 120 --no-semi --tab-width 2 --single-quote --write '{src,tests}/**/*.js'"
  },
  "keywords": [
    "mobx",
    "router",
    "react"
  ],
  "license": "ISC",
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "path-to-regexp": "~1.2.1",
    "prop-types": "^15.6.0",
    "query-string": "^5.1.0"
  },
  "peerDependencies": {
    "mobx": "^3.x",
    "mobx-react": "^4.x",
    "react": "16.x"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "del-cli": "^1.1.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^4.10.0",
    "husky": "^0.14.3",
    "jest": "^21.3.0-beta.14",
    "jest-cli": "^22.4.2",
    "lint-staged": "^6.0.0",
    "prettier": "^1.7.4",
    "raf": "^3.4.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "jest": {
    "setupFiles": [
      "./tests/jestsetup.js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
  "lint-staged": {
    "*.js": [
      "prettier --print-width 120 --tab-width 2 --single-quote --write",
      "git add"
    ]
  }
}
